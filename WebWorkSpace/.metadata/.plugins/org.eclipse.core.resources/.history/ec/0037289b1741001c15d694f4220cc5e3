<%
	//값 받기
	int x = Integer.parseInt(request.getParameter("x")); 
	int y = Integer.parseInt(request.getParameter("y"));
	
	int hab = x+y;
	int cha = x-y;
	
	public int getGon(int a, int b){
		return a * b;
	}
%>

<h1><%= x %> + <%= y %> = <%= hab %></h1>
<h1><%= x %> - <%= y %> = <%= cha %></h1>
